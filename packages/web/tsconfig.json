{
  "compilerOptions": {
    "outDir": "dist/esm",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "types": [
      "vite/client"
    ],
    "paths": {
      "@semantic-api/common": [
        "./node_modules/@semantic-api/common/dist"
      ],
      "@semantic-api/common/*": [
        "./node_modules/@semantic-api/common/dist/*"
      ],
      "@semantic-api/types": [
        "./node_modules/@semantic-api/types/dist"
      ],
      "@semantic-api/types/*": [
        "./node_modules/@semantic-api/types/dist/*"
      ],
      "@semantic-api/system": [
        "./node_modules/@semantic-api/system/dist"
      ]
    }
  }
}
