{
  "collection": "feedback",
  "route": true,
  "strict": true,
  "unicon": "comment-alt-dots",
  "presets": [
    "owned",
    "deleteAll",
    "view"
  ],
  "individualActions": {
    "delete": {
      "name": "Remover",
      "unicon": "trash-alt",
      "ask": true
    }
  },
  "formLayout": {
    "section": {
      "span": 4
    },
    "type": {
      "span": 2
    }
  },
  "fields": {
    "product_version": {
      "label": "Versão do produto",
      "type": "text",
      "noform": true
    },
    "base_version": {
      "label": "Versão da base",
      "type": "text",
      "noform": true
    },
    "user_agent": {
      "label": "User-Agent",
      "type": "text",
      "notable": true,
      "noform": true
    },
    "section": {
      "label": "Seção",
      "type": "text"
    },
    "type": {
      "label": "Tipo",
      "type": "select",
      "required": true,
      "values": [
        "Dúvida",
        "Sugestão",
        "Elogio",
        "Bug report",
        "Outra coisa"
      ]
    },
    "comment": {
      "label": "Comentário",
      "type": "textbox",
      "notable": true
    }
  }
}
