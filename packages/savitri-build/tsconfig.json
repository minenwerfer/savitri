{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "braun/*": [
        "./node_modules/braun/dist/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
